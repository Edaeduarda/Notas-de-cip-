// FIM CIRCULAÇÃO ACIDO CIP ROTINA
if (flow.get('CIP_PASSO_TINAS') == 5 && flow.get('CIP_RECEITA_TINAS') == 3 && flow.get('CIP_ADF_ETAPA_ACIDO_INICIADA') == true) {
