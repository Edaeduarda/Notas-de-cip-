// LISTA CONCENTRAÇÃO SANITIZANTE CIP PERIODICO

if (flow.get('CIP_PASSO_TINAS') == 6 && flow.get('CIP_RECEITA_TINAS') == 2 && flow.get('CIP_ADF_ETAPA_SANITIZANTE_INICIADA') == true) {

