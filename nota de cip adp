    flow.set('LISTA_CIP_ADP_CONCENTRACAO_SANITIZANTE_PERIODICO',[]);
    flow.set('NOTA_CIP_ADP_CONCENTRACAO_SANITIZANTE_PERIODICO')
    
    // NOTA
    flow.set('CIP_ADP_NOTA_ETAPA_TEMPO_SANITIZANTE', notaEtapaTempo('sanitizante'));
