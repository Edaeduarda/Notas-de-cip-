// FIM CIRCULAÇÃO SANITIZANTE CIP PERIODICO
if(flow.get('CIP_ADP_PASSO') == 7 && flow.get('CIP_ADP_RECEITA') == 1 && flow.get('CIP_ADP_ETAPA_SANITIZANTE_INICIADA') == true ){
